mc <- tukey_interaccion$groups %>%
rownames_to_column("trat") %>%
separate(trat, c("riego", "variedad"))
str (mc)
library(ggplot2)
# Crear el gráfico de barras con ggplot2
ggplot(mc, aes(x = variedad, y = lfa, fill = riego)) +
